@ARTICLE{marascuilo_appropriate_1970,
  author = {Marascuilo, Leonard A and Levin, Joel R},
  title = {Appropriate Post Hoc Comparisons for Interaction and Nested Hypotheses
	in Analysis of Variance Designs: The Elimination of Type {IV} Errors},
  journal = {American Educational Research Journal},
  year = {1970},
  volume = {7},
  pages = {397--421},
  number = {3},
  abstract = {In this paper, the concept of Type {IV} errors, or incorrect interpretations
	of correctly rejected hypotheses, is introduced and illustrated.
	Discussion focuses upon the commonly confused post hoc comparisons
	of interaction and nested hypotheses in analysis of variance designs.
	Models for both designs are outlined and examples applicable to each
	are presented. In these examples, appropriate contrasts and error
	terms are specified. It is further demonstrated that the statistical
	decisions arrived at under each model do not necessarily coincide.
	Finally, it is suggested that the onus of responsibility for choice
	between the two designs be placed on the individual investigator,
	with the decision being contingent upon the kinds of inferences he
	wishes to draw.},
  doi = {10.2307/1161635},
  issn = {0002-8312},
  shorttitle = {Appropriate Post Hoc Comparisons for Interaction and Nested Hypotheses
	in Analysis of Variance Designs},
  url = {http://www.jstor.org/stable/1161635}
}

@ARTICLE{meyer_misinterpretation_1991,
  author = {Meyer, D L},
  title = {Misinterpretation of interaction effects: a reply to {Rosnow} and {Rosenthal}},
  journal = {Psychological Bulletin},
  year = {1991},
  volume = {110},
  pages = {571--573; discussion 574--576},
  number = {3},
  abstract = {The structural model of analysis of variance for multiway tables,
	with main effects and interaction parameters, is overspecified. Only
	the set of estimable functions of the cell means is useful. The individual
	estimates of the parameters are artificial as regards the underlying
	scientific process. Rosnow and Rosenthal (1989a) stated that it is
	"absolutely necessary" (pg 146) that interactions should be interpreted
	by examining the usual estimates of the interaction parameters. This
	is incorrect.},
  issn = {0033-2909},
  keywords = {Analysis of Variance, Humans, Mental Disorders, Models, Psychometrics,
	Statistical},
  shorttitle = {Misinterpretation of interaction effects},
  url = {http://www.ncbi.nlm.nih.gov/pubmed/1758924}
}

@ARTICLE{nelder_reformulation_1977,
  author = {Nelder, J. A.},
  title = {A Reformulation of Linear Models},
  journal = {Journal of the Royal Statistical Society. Series A {(General)}},
  year = {1977},
  volume = {140},
  pages = {48--77},
  number = {1},
  abstract = {Dissatisfaction is expressed with aspects of the current exposition
	of linear models, including the neglect of marginality, unnecessary
	differences between models for finite and infinite populations, failure
	to distinguish different kinds of random terms, imposition of unnecessary
	and inconsistent constraints on parameters, and lack of an adequate
	notation for negative components of variance. The reformulation,
	exemplified for crossed and nested classifications of balanced data,
	and for simple orthogonal designed experiments, is designed to integrate
	finite and infinite populations, random and fixed effects, excess
	and deficit of variance, to avoid unnecessary constraints on parameters,
	and to lead naturally to interesting hypotheses about the model terms.},
  doi = {10.2307/2344517},
  issn = {0035-9238},
  url = {http://www.jstor.org/stable/2344517}
}

